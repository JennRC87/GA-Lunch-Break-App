
  <body>
  <%= render 'layouts/header' %>
    <input id="pac-input" class="controls" type="text"
        placeholder="Enter a location">


    <div id="map"></div>

<p id="notice"><%= notice %></p>

<br>

<h1>GA Students Recommendations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.name %></td>
        <th colspan="3"></th>
        <td><%= place.address %></td>
        <th colspan="3"></th>
        <td><%= link_to 'Details', place %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= render 'layouts/footer' %>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBAchzUgA7nscKvpxt3v_YoC1pn5_55GcM&libraries=places&callback=initMap" async defer></script>

  </body>
